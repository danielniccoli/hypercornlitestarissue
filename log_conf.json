{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "default": {
            "()": "uvicorn.logging.DefaultFormatter",
            "fmt": "%(levelprefix)s %(asctime)s | %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        },
        "access": {
            "()": "uvicorn.logging.AccessFormatter",
            "fmt": "%(levelprefix)s %(asctime)s %(client_addr)s - \"%(request_line)s\" %(status_code)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout",
            "formatter": "default"
        },
        "access_console": {
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout",
            "formatter": "access"
        }
    },
    "root": {
        "level": "TRACE",
        "handlers": [
            "console"
        ]
    },
    "loggers": {
        "uvicorn": {
            "level": "TRACE",
            "handlers": [
                "console"
            ],
            "propagate": false
        },
        "uvicorn.error": {
            "level": "TRACE",
            "handlers": [
                "console"
            ],
            "propagate": false
        },
        "uvicorn.access": {
            "level": "TRACE",
            "handlers": [
                "access_console"
            ],
            "propagate": false
        }
    }
}